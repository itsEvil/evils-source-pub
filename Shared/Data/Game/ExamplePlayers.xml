<Objects>
	<!-- 
		*** Inventory ***
	Filled with items starting from top to bottom 
	If size is greater then items provided then 
	the rest of the slots will be filled with 
	copies of item from the empty attribute
	-->
	
	
	<Object id="100" name="Example Class">
		<Class>Player</Class>
		<Inventory size="20" empty="1" emptyStack="0">
			<Item id="200" stack="1"/>
		</Inventory>
	</Object>
</Objects>